# GRAFICO 1
ggplot() +
geom_bar(data = penguins, #<1>
aes(x = species)) + #<1>
geom_hline(yintercept = 75, #<2>
color = "red") + #<2>
theme_minimal() #<3>
# GRAFICO 2
ggplot() +
geom_hline(yintercept = 75,
color = "red") +
geom_bar(data = penguins,
aes(x = species)) +
theme_minimal()
ggplot(penguins) +
geom_point(aes(x = flipper_length_mm,
y = body_mass_g)) +
theme_minimal()
penguins %>%
ggplot(aes(x = flipper_length_mm,
y = body_mass_g)) +
geom_point(aes(color = species)) +
geom_smooth(
method = "lm",
se = FALSE) +
theme_minimal()
penguins %>%
ggplot(aes(x = flipper_length_mm,
y = body_mass_g,
color = species)) +
geom_point() +
geom_smooth(
method = "lm",
se = FALSE) +
theme_minimal()
ggplot(penguins) +
geom_point(aes(x = flipper_length_mm,
y = body_mass_g,
color = species)) +
theme_minimal()
penguins %>%
ggplot(aes(x = flipper_length_mm,
y = body_mass_g)) +
geom_point(aes(color = species)) +
geom_smooth(
method = "lm",
se = FALSE) +
theme_minimal()
